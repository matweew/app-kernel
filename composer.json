{
  "name": "spryker/app-kernel",
  "type": "library",
  "description": "AppKernel module",
  "license": "proprietary",
  "require": {
    "php": ">=8.0",
    "spryker/message-broker": "^1.8",
    "spryker/translator": "^1.11"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "^3.49"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/",
      "SprykerTest\\Glue\\App\\Helper\\": "tests/SprykerTest/Glue/App/_support/Helper/",
      "SprykerTest\\Zed\\App\\Helper\\": "tests/SprykerTest/Zed/Aüü/_support/Helper/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
